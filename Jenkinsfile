pipeline {
    agent any
    
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World this is Raj using Jenkinsfile!'
            }
        }    
        stage('Build') {
            steps {
                echo 'Building stage in progress!'
            }
        }    
        stage('Deploy') {
            steps {
                echo 'Deploying stage in progress!'
            }
        }    
        stage('Test') {
            steps {
                echo 'Testing stage in progress!'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing stage in progress!'
            }
        }    
    }
}
